{
  "name": "puppeteer-ie",
  "version": "0.5.5",
  "descripton": "Headless Internet Explorer NodeJS API inspired by Puppeteer.",
  "keywords": [
    "internet-explorer",
    "headless-browsers",
    "activex-control",
    "puppeteer"
  ],
  "author": "shiy007@qq.com",
  "license": "LGPL-3.0",
  "homepage": "https://techquery.github.io/Puppeteer-IE/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechQuery/Puppeteer-IE.git"
  },
  "bugs": {
    "url": "https://github.com/TechQuery/Puppeteer-IE/issues"
  },
  "main": "source/index.js",
  "engines": {
    "node": "^7.6.0"
  },
  "os": [
    "win32"
  ],
  "scripts": {
    "lint": "eslint ./source/ ./test/ --fix",
    "document": "jsdoc ./source/ -c ./build/document.json",
    "clean": "cd ./docs/  &&  reduce-blank  &&  node ../build/beautifyDoc",
    "build": "npm run lint  &&  npm run document  &&  npm run clean",
    "help": "web-server docs/ -o",
    "check": "mocha -r should --recursive --no-timeouts -c --inline-diffs",
    "test": "npm run check -- --exit",
    "debug": "npm run check -- --inspect-brk",
    "prepublishOnly": "npm run build",
    "precommit": "npm run build  &&  git add ."
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@tech_query/node-toolkit": "^0.6.1",
    "cookie": "^0.3.1",
    "html2canvas": "^0.5.0-beta4",
    "winax": "^1.0.14"
  },
  "devDependencies": {
    "docdash": "^1.0.0",
    "eslint": "^5.6.0",
    "fs-extra": "^7.0.0",
    "fs-match": "^0.2.1",
    "husky": "^0.14.3",
    "js-beautify": "^1.8.6",
    "jsdoc": "^3.5.5",
    "jsdoc-inheritance-diagram": "^1.2.1",
    "koapache": "^1.0.4",
    "mocha": "^5.2.0",
    "reduce-blank": "^0.2.0",
    "should": "^13.2.3"
  }
}
