{
    "opts":         {
        "recurse":        true,
        "readme":         "ReadMe.md",
        "template":       "node_modules/docdash",
        "destination":    "docs/",
        "verbose":        true
    },
    "plugins":      [
        "plugins/markdown", "node_modules/jsdoc-inheritance-diagram"
    ],
    "markdown":     {
        "hardwrap":    true
    },
    "docdash":      {
        "search":  true,
        "menu":    {
            "Source code":  {
                "href":    "https://github.com/TechQuery/Puppeteer-IE",
                "target":  "_blank"
            }
        }
    }
}
